
-- SCHEMA: olx_desafio4

-- DROP SCHEMA olx_desafio4 ;

CREATE SCHEMA olx_desafio4
    AUTHORIZATION postgres;

CREATE SEQUENCE olx_desafio4.palavras_id_seq
    INCREMENT 1
    START 1
    MINVALUE 1
    MAXVALUE 9223372036854775807
    CACHE 1;

ALTER SEQUENCE olx_desafio4.palavras_id_seq
    OWNER TO postgres;

-- Table: olx_desafio4.palavras

-- DROP TABLE olx_desafio4.palavras;

CREATE TABLE olx_desafio4.palavras
(
    id bigint NOT NULL DEFAULT nextval('olx_desafio4.palavras_id_seq'::regclass),
    termo character varying(4000) NOT NULL,
    CONSTRAINT palavras_pkey PRIMARY KEY (id),
    CONSTRAINT ck_termo UNIQUE (termo)
)
WITH (
    OIDS = FALSE
)
TABLESPACE pg_default;

ALTER TABLE olx_desafio4.palavras
    OWNER to postgres;